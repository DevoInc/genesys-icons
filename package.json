{
  "name": "@devoinc/genesys-icons",
  "version": "7.0.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "npm run storybook",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "generate": "npm run generate:font && npm run generate:js",
    "generate:font": "tsx ./scripts/font/generate.ts",
    "generate:react": "tsx ./scripts/react/generate.ts",
    "generate:js": "tsx ./scripts/js/generate.ts",
    "validations": "npm run validations:iconName",
    "validations:iconName": "tsx ./scripts/validations/iconName/checkIconName.ts",
    "lint": "oxlint -c oxlintrc.json",
    "build": "tsup",
    "clean": "rm -rf dist src/icons/GI* src/icons/index.ts",
    "test": "vitest",
    "test:ci": "vitest --coverage",
    "stryker": "npx stryker run",
    "dist": "npm run clean && npm run generate:react && npm run build && npm run generate",
    "release": "release-it"
  },
  "devDependencies": {
    "@floating-ui/react": "^0.27.8",
    "@popperjs/core": "^2.11.8",
    "@release-it/conventional-changelog": "^10.0.1",
    "@storybook/addon-a11y": "^8.6.11",
    "@storybook/addon-actions": "^8.6.11",
    "@storybook/addon-docs": "^8.6.11",
    "@storybook/addon-essentials": "^8.6.11",
    "@storybook/addon-links": "^8.6.11",
    "@storybook/node-logger": "^8.6.12",
    "@storybook/react": "^8.6.11",
    "@storybook/react-vite": "^8.6.11",
    "@stryker-mutator/core": "^8.7.1",
    "@stryker-mutator/typescript-checker": "^8.7.1",
    "@stryker-mutator/vitest-runner": "^8.7.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.15.17",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.3",
    "@vitest/coverage-v8": "^3.1.3",
    "ahooks": "^3.8.4",
    "cheerio": "^1.0.0",
    "decamelize": "^6.0.0",
    "domhandler": "^5.0.3",
    "happy-dom": "^17.4.7",
    "oxlint": "^0.16.10",
    "pascal-case": "^3.1.2",
    "prettier": "^3.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.1.0",
    "react-popper": "^2.3.0",
    "react-window": "^1.8.11",
    "release-it": "^19.0.2",
    "sass": "^1.88.0",
    "storybook": "^8.6.12",
    "styled-components": "^6.1.18",
    "svgtofont": "^6.3.2",
    "terser": "^5.39.0",
    "tsup": "^8.4.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.1.3"
  },
  "overrides": {
    "esbuild": "0.24.0",
    "ahooks": {
      "react": "$react"
    },
    "react-popper": {
      "react": "$react",
      "react-dom": "$react-dom"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=7"
  },
  "optionalDependencies": {
    "react": "^16.3.0 || 17.x || 18.x || 19.x"
  }
}
